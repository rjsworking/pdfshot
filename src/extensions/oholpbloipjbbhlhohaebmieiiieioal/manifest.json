{
   "author": "Daniel Kladnik @ kiboke studio",
   "background": {
      "scripts": [ "data/rules.js", "data/context-menu.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/16.png",
         "32": "icons/32.png"
      },
      "default_popup": "data/menu/index.html"
   },
   "default_locale": "en",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extensionName__",
   "options_ui": {
      "chrome_style": true,
      "page": "data/options.html"
   },
   "permissions": [ "tabs", "storage", "http://*/*", "https://*/*", "notifications", "webRequest", "webRequestBlocking", "webNavigation" ],
   "short_name": "__MSG_extensionDescription__",
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "3.4.4"
}
